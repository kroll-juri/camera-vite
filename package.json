{
  "name": "common",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf ./dist && tsc -b && vite build",
    "lint": "eslint --fix --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx . --max-warnings 0",
    "lint-staged": "lint-staged",
    "preview": "vite preview",
    "prettify": "prettier --cache --write \"src/**/*.{ts,tsx,js,json,yml,scss,html}\" --ignore-path .prettierignore",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/swiper": "^6.0.0",
    "axios": "^1.9.0",
    "install": "^0.13.0",
    "jsdoc": "^4.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "swiper": "^11.2.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.3.4",
    "dayjs": "^1.11.13",
    "eslint": "^9.22.0",
    "eslint-config-htmlacademy": "^10.0.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "react-focus-lock": "^2.13.6",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
